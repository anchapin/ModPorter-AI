{
  "name": "modporter-ai-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "lint": "eslint . --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --reporter=verbose",
    "test:ci": "vitest run --reporter=verbose --passWithNoTests || echo Tests completed with exit code %errorlevel%",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:install": "playwright install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:test": "test-storybook",
    "storybook:build": "storybook build --output-dir dist-storybook"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "axios": "^1.13.2",
    "date-fns": "^4.1.0",
    "mermaid": "^11.12.1",
    "monaco-editor": "^0.54.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@storybook/addon-docs": "^10.0.5",
    "@storybook/addon-onboarding": "^10.0.5",
    "@storybook/react-vite": "^10.0.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-storybook": "^10.0.5",
    "globals": "^16.5.0",
    "jsdom": "^27.1.0",
    "msw": "^2.12.0",
    "patch-package": "^8.0.0",
    "playwright": "^1.49.0",
    "@playwright/test": "^1.49.0",
    "prettier": "^3.6.2",
    "storybook": "^10.0.5",
    "typescript": "^5.9.3",
    "vite": "7.2.2",
    "vitest": "^4.0.7",
    "web-streams-polyfill": "^4.2.0"
  }
}
