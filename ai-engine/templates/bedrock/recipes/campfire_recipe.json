{
  "format_version": "1.20.10",
  "minecraft:recipe_campfire_cooking": {
    "description": {
      "identifier": "{{ namespace }}:{{ recipe_name }}"
    },
    "tags": ["campfire"],
    "input": {
      "item": "{{ input.item }}",
      "data": {{ input.data | default(0) }}
    },
    "output": "{{ output.item }}",
    "output_count": {{ output.count | default(1) }}{% if cooking_time %},
    "cookingtime": {{ cooking_time }}{% endif %}{% if experience %},
    "experience": {{ experience }}{% endif %}
  }
}
