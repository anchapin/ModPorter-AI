{
  "format_version": "1.20.10",
  "minecraft:recipe_stonecutter": {
    "description": {
      "identifier": "{{ namespace }}:{{ recipe_name }}"
    },
    "tags": ["stonecutter"],
    "input": {
      "item": "{{ input.item }}",
      "data": {{ input.data | default(0) }}
    },
    "output": {
      "item": "{{ output.item }}",
      "data": {{ output.data | default(0) }},
      "count": {{ output.count | default(1) }}
    }
  }
}
